{
  "name": "react-h5-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=4096 vite",
    "serve": "pnpm dev",
    "build": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 tsc -b && vite build",
    "build:staging": "rimraf dist && vite build --mode staging",
    "report": "rimraf dist && vite build",
    "preview": "vite preview",
    "preview:build": "pnpm build && vite preview",
    "typecheck": "tsc --noEmit",
    "svgo": "svgo -f . -r",
    "clean:cache": "rimraf .eslintcache && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm store prune && pnpm install",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,mock,build}/**/*.{ts,tsx,js,jsx}\" \"**/*.md\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,css,scss,less}\" --cache-location node_modules/.cache/stylelint/",
    "lint:markdown": "markdownlint \"**/*.md\" --fix",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint && pnpm lint:markdown",
    "fix:markdown": "node scripts/fix-markdown.js",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "react",
    "h5",
    "mobile",
    "typescript",
    "vite",
    "admin",
    "pwa"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@pureadmin/utils": "^2.6.2",
    "@tanstack/react-query": "^5.62.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "ahooks": "^3.8.1",
    "antd": "^5.23.3",
    "antd-mobile": "^5.39.0",
    "axios": "^1.11.0",
    "china-area-data": "^5.0.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.1.6",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "qs": "^6.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.1.4",
    "react-use": "^17.5.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.10",
    "responsive-storage": "^2.2.0",
    "swiper": "^11.2.10",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@iconify/json": "^2.2.364",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.9",
    "@types/qs": "^6.14.0",
    "@types/react-virtualized": "^9.21.30",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "boxen": "^8.0.1",
    "code-inspector-plugin": "^1.0.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "stylelint": "^16.23.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "unocss": "^66.4.0",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.0.6",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fake-server": "^2.2.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-router-warn": "^1.0.0",
    "vite-svg-loader": "^5.1.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.11.0",
    "pnpm": ">=9"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "are-we-there-yet": "*",
      "sourcemap-codec": "*",
      "lodash.isequal": "*",
      "domexception": "*",
      "w3c-hr-time": "*",
      "inflight": "*",
      "npmlog": "*",
      "rimraf": "*",
      "stable": "*",
      "gauge": "*",
      "abab": "*",
      "glob": "*"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "es5-ext",
      "esbuild"
    ]
  }
}
